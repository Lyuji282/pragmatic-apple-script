tell application "System Events"
  set x to 0.1
  keystroke "l" using {command down}
  delay x
  keystroke "c" using {command down}
  delay x 
  keystroke "w" using {command down}
  delay x
  keystroke "n" using {command down}
  delay x
  keystroke "v" using {command down}
  delay x
  keystroke return
end tell